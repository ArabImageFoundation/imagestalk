{
  "name": "AIF",
  "version": "0.0.1",
  "description": "Website for Arab Image Foundation pre-planning",
  "main": "index.js",
  "dependencies": {
    "harp": "0.19.0",
    "jstransformer-marked": "^1.0.1",
    "nib": "^1.1.0"
  },
  "devDependencies": {},
  "scripts": {
    "start": "NODE_ENV=development ./node_modules/.bin/harp server ./public",
    "compile": "NODE_ENV=production ./node_modules/.bin/harp compile ./public ./www",
    "update": "git checkout master && git add -A && git commit && git push && git subtree push --prefix www origin gh-pages",
    "compileUpdate": "npm run compile && npm run update",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ArabImageFoundation/imagestalk.git"
  },
